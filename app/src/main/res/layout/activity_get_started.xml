<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/secondaryColor"
    tools:context=".GetStartedActivity" >

    <Button
        android:id="@+id/signupNavigate"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/doctorCard"
        android:text="@string/signUp"
        android:textColor="@color/GetStartedBtn"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView33" />

    <Button
        android:id="@+id/loginNavigate"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/doctorCard"
        android:text="@string/login"
        android:textColor="@color/GetStartedBtn"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signupNavigate" />

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="326dp"
        android:layout_height="168dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/medlogo" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="175dp"
        android:layout_height="249dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView37"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.266"
        app:srcCompat="@drawable/baseline_circle"
        tools:layout_editor_absoluteX="324dp" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="175dp"
        android:layout_height="249dp"
        android:layout_marginStart="200dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView37"
        app:srcCompat="@drawable/baseline_circle"
        tools:layout_editor_absoluteY="389dp" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="175dp"
        android:layout_height="249dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.734"
        app:srcCompat="@drawable/baseline_circle" />

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="360dp"
        android:layout_height="303dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/twodocs" />
</androidx.constraintlayout.widget.ConstraintLayout>